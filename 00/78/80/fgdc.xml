        <attrdomv>
          <udom>Free text.</udom>
        </attrdomv>
      </attr>
      <attr>
        <attrlabl>MODIFIED</attrlabl>
        <attrdef>Date the municipal boundary was last modified.</attrdef>
        <attrdefs>Tompkins County ITS GIS Division, 2002.</attrdefs>
        <attrdomv>
          <udom>Either a date (YYYY-MM-DD) or "Null".</udom>
        </attrdomv>
      </attr>
      <attr>
        <attrlabl>FIPS1990</attrlabl>
