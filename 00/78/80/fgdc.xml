        <attrdef>Date that the municipality was incorporated.</attrdef>
        <attrdefs>Tompkins County ITS GIS Division, 2002.</attrdefs>
        <attrdomv>
          <udom>Either a date (YYYY-MM-DD) or "Null".</udom>
        </attrdomv>
      </attr>
      <attr>
        <attrlabl>VALIDFROM</attrlabl>
        <attrdef>Historical date that the municipal boundary was first validated.</attrdef>
        <attrdefs>Tompkins County ITS GIS Division, 2002.</attrdefs>
        <attrdomv>
          <udom>Either a date (YYYY-MM-DD) or "Null".</udom>
        </attrdomv>
      </attr>
      <attr>
        <attrlabl>VALIDTO</attrlabl>
        <attrdef>Historical date that the municipality was last validated.</attrdef>
        <attrdefs>Tompkins County ITS GIS Division, 2002.</attrdefs>
        <attrdomv>
          <udom>All values are "9999-01-01".</udom>
        </attrdomv>
      </attr>
      <attr>
        <attrlabl>TRAVERSE</attrlabl>
