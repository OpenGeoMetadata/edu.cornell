        <attrdefs>Tompkins County ITS GIS Division, 2002.</attrdefs>
        <attrdomv>
          <udom>A five-digit number.</udom>
        </attrdomv>
      </attr>
      <attr>
        <attrlabl>SWIS</attrlabl>
